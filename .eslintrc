{
  "extends": ["airbnb-base", "airbnb-typescript/base", "prettier"],
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "root": true,
  "ignorePatterns": ["dist"],
  "rules": {
    "import/prefer-default-export": ["off"],
    "@typescript-eslint/no-unused-vars": [
      "warn",
      { "varsIgnorePattern": "^_", "argsIgnorePattern": "^_" }
    ]
  }
}
